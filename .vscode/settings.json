{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/*.js": { "when": "$(basename).ts" }
    },
    "typescript.inlayHints.parameterNames.enabled": "all",
    "search.useGlobalIgnoreFiles": false,
    "search.useIgnoreFiles": false,
    "files.eol": "\n",
    "liveServer.settings.port": 5500
}
